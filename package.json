    "@tsed/engines": "^8.14.1",
    "@tsed/exceptions": "^8.14.1",
    "@tsed/json-mapper": "^8.14.1",
    "@tsed/logger": "^7.0.2",
    "@tsed/openspec": "^8.14.1",
    "@tsed/platform-cache": "^8.14.1",
    "@tsed/platform-exceptions": "^8.14.1",
    "@tsed/platform-express": "^8.14.1",
    "@tsed/platform-http": "^8.14.1",
    "@tsed/platform-log-request": "^8.14.1",
