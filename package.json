{
  "name": "tsed-docker-mysql-instance",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run barrels && tsc",
    "barrels": "barrels",
    "start": "npm run barrels && nodemon src/index.ts",
